<h1>Lists#index</h1>
<%= link_to "Add new list", new_list_path, class: "btn btn-primary" %>
<%= link_to "Playground", playground_path, class: "btn btn-danger" %>
<ul class="list-group">
  <% @lists.each do |list| %>
    <li class="list-group"><%= link_to list.name, list_path(list.id) %></li>
  <% end %>
</ul>
